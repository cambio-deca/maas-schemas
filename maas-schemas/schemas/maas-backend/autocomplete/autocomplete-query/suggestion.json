{
  "$id": "http://maasglobal.com/maas-backend/autocomplete/autocomplete-query/suggestion.json",
  "description": "Response schema for autocomplete suggestion",
  "type": "object",
  "properties": {
    "label": {
      "type": "string",
      "example": "Nyon, Switzerland"
    },
    "addressId": {
      "type": "string",
      "example": "rrPlaceId%3Ape--Nyon--whosonfirst%3Alocality%3A101914095"
    },
    "meta": {
      "type": "object",
      "google": { "$ref": "#/definitions/googleMeta" },
      "routerank": { "$ref": "#/definitions/routerankMeta" }
    }
  },
  "required": ["label", "meta"],
  "additionalProperties": false,
  "definitions": {
    "googleMeta": {
      "type": "object",
      "properties": {
        "placeId": {
          "type": "string",
          "example": "ChIJzU8-ZyCojkcRo3MctwyaeMM"
        }
      }
    },
    "routerankMeta": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "example": "pe--Nyon--whosonfirst:locality:101914095"
        },
        "lat": {
          "$ref": "http://maasglobal.com/core/components/units-geo.json#/definitions/relaxedLatitude"
        },
        "lon": {
          "$ref": "http://maasglobal.com/core/components/units-geo.json#/definitions/relaxedLongitude"
        }
      }
    }
  }
}
