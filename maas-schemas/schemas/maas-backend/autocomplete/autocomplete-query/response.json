{
  "$id": "http://maasglobal.com/maas-backend/autocomplete/autocomplete-query/response.json",
  "description": "Response schema for autocomplete",
  "type": "object",
  "properties": {
    "suggestions": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "debug": {
      "type": "object"
    }
  },
  "required": ["suggestions"],
  "additionalProperties": false,
  "definitions": {
    "suggestion": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "example": "pe--Nyon--whosonfirst:locality:101914095"
        },
        "label": {
          "type": "string",
          "example": "Nyon, Switzerland"
        },
        "lat": { "$ref": "http://maasglobal.com/core/components/units-geo.json#/definitions/relaxedLatitude" },
        "lon": { "$ref": "http://maasglobal.com/core/components/units-geo.json#/definitions/relaxedLongitude" },
        "addressId": {
          "type": "string",
          "example": "rrPlaceId%3Ape--Nyon--whosonfirst%3Alocality%3A101914095"
        }
      },
      "required": ["label", "lat", "lon"],
      "additionalProperties": false
    }
  }
}
