{
  "$id": "http://maasglobal.com/maas-backend/autocomplete/autocomplete-query/response.json",
  "description": "Response schema for autocomplete",
  "type": "object",
  "properties": {
    "provider": {
      "type": "string",
      "description": "The autocomplete provider used to return the results",
      "example": "google"
    },
    "suggestions": { "$ref": "#/definitions/suggestionsList" },
    "debug": {
      "type": "object"
    }
  },
  "required": ["provider", "suggestions"],
  "additionalProperties": false,
  "definitions": {
    "suggestionsList": {
      "type": "array",
      "items": {
        "$ref": "http://maasglobal.com/maas-backend/autocomplete/autocomplete-query/suggestion.json"
      }
    }
  }
}
